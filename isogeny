#!/usr/bin/env bb
;; -*- mode: clojure -*-

(require '[babashka.classpath :as cp]
         '[babashka.fs :as fs])

(-> *file*
    fs/real-path
    fs/parent
    (fs/file "src")
    str
    cp/add-classpath)

(require '[dcprevere.isogeny :as iso])
(apply iso/-main *command-line-args*)
(System/exit 0)
